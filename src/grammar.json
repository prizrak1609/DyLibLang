{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "dyliblang",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "load_statement"
        },
        {
          "type": "SYMBOL",
          "name": "unload_statement"
        },
        {
          "type": "SYMBOL",
          "name": "variable_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        }
      ]
    },
    "load_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "load"
        },
        {
          "type": "FIELD",
          "name": "library",
          "content": {
            "type": "SYMBOL",
            "name": "library_identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "as"
                },
                {
                  "type": "FIELD",
                  "name": "alias",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_alias_identifier"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "unload_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "unload"
        },
        {
          "type": "FIELD",
          "name": "library",
          "content": {
            "type": "SYMBOL",
            "name": "library_identifier"
          }
        }
      ]
    },
    "variable_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "_variable_identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "type"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value_declaration"
          }
        }
      ]
    },
    "value_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "SYMBOL",
          "name": "function_reference"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "function_name",
          "content": {
            "type": "SYMBOL",
            "name": "_function_identifier"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "argument",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_argument_declaration"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "FIELD",
                        "name": "argument",
                        "content": {
                          "type": "SYMBOL",
                          "name": "_argument_declaration"
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "function_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "function_name",
          "content": {
            "type": "SYMBOL",
            "name": "_function_identifier"
          }
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "FIELD",
          "name": "symbol",
          "content": {
            "type": "SYMBOL",
            "name": "_symbol_identifier"
          }
        }
      ]
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "-"
                  },
                  {
                    "type": "STRING",
                    "value": "+"
                  }
                ]
              },
              {
                "type": "PATTERN",
                "value": "[0-9]+"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "-"
                  },
                  {
                    "type": "STRING",
                    "value": "+"
                  }
                ]
              },
              {
                "type": "PATTERN",
                "value": "[0-9]+\\.[0-9]+"
              }
            ]
          }
        ]
      }
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "int"
        },
        {
          "type": "STRING",
          "value": "void"
        },
        {
          "type": "STRING",
          "value": "char"
        },
        {
          "type": "STRING",
          "value": "float"
        },
        {
          "type": "STRING",
          "value": "double"
        },
        {
          "type": "STRING",
          "value": "short"
        },
        {
          "type": "STRING",
          "value": "long"
        },
        {
          "type": "STRING",
          "value": "unsigned"
        },
        {
          "type": "STRING",
          "value": "signed"
        },
        {
          "type": "STRING",
          "value": "bool"
        },
        {
          "type": "STRING",
          "value": "size_t"
        },
        {
          "type": "STRING",
          "value": "ptrdiff_t"
        },
        {
          "type": "STRING",
          "value": "wchar_t"
        },
        {
          "type": "STRING",
          "value": "int8_t"
        },
        {
          "type": "STRING",
          "value": "int16_t"
        },
        {
          "type": "STRING",
          "value": "int32_t"
        },
        {
          "type": "STRING",
          "value": "int64_t"
        },
        {
          "type": "STRING",
          "value": "uint8_t"
        },
        {
          "type": "STRING",
          "value": "uint16_t"
        },
        {
          "type": "STRING",
          "value": "uint32_t"
        },
        {
          "type": "STRING",
          "value": "uint64_t"
        },
        {
          "type": "SYMBOL",
          "name": "cpp_type"
        },
        {
          "type": "SYMBOL",
          "name": "function_type"
        }
      ]
    },
    "cpp_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "std"
        },
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "SYMBOL",
          "name": "_cpp_type"
        }
      ]
    },
    "function_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "return_type",
          "content": {
            "type": "SYMBOL",
            "name": "type"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "type",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_argument_type"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "FIELD",
                        "name": "type",
                        "content": {
                          "type": "SYMBOL",
                          "name": "_argument_type"
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "library_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_\\.]*"
    },
    "_alias_identifier": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "alias_identifier"
    },
    "_variable_identifier": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "variable_identifier"
    },
    "_function_identifier": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "function_identifier"
    },
    "_symbol_identifier": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "symbol_identifier"
    },
    "_cpp_type": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "cpp_type"
    },
    "_argument_type": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "type"
      },
      "named": true,
      "value": "argument_type"
    },
    "_argument_declaration": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "argument_declaration"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [
    "value_declaration",
    "type"
  ],
  "supertypes": [
    "statement"
  ]
}
